    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Marketdata",
            "request": "launch",
            "mainClass": "com.metatech.crypto.exchange.Marketdata",
            "projectName": "metatech-crypto-exchange"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "args": "-DCONFIG='./cnf/exchange.cnf.xml' -DTARGET='coincheck'"
        },
        {
            "type": "java",
            "name": "Java with Arguments Prompt",
            "request": "launch",
            "mainClass": "com.metatech.JavaCat.Marketdata",
            "args": "${command:SpecifyProgramArgs}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}